<?xml version='1.0' encoding='utf-8'?>
<widget id="com.terikon.PhotoLibraryDemoPhotoSwipe" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Upload Fotos</name>
    <description>
        backup de multiplas fotos.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <resource dest="www/lib">
        <resource src="node_modules">
            <resource dest="photoswipe" src="photoswipe/dist" />
        </resource>
    </resource>
    <engine name="android" spec="~6.1.2" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-camera" spec="2.4.1" />
    <plugin name="cordova-plugin-file-transfer" spec="1.6.3" />
    <plugin name="cordova-plugin-file" spec="4.3.3" />
    <plugin name="cordova-plugin-image-picker" spec="git+https://github.com/wymsee/cordova-imagePicker.git" />
    <plugin name="cordova-plugin-device" spec="~1.1.7" />
    <plugin name="cordova-plugin-imagepicker" spec="1.3.0" />
    <plugin name="cordova-plugin-photo-library" spec="~2.0.2">
        <variable name="PHOTO_LIBRARY_USAGE_DESCRIPTION" value="To choose photos" />
    </plugin>
    <plugin name="cordova-plugin-copy-resources" spec="~0.0.0" />
</widget>
